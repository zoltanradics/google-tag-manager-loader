{
  "name": "@zoltanradics/gtm-loader",
  "version": "1.0.1",
  "description": "Google Tag Manager (GTM) script loader",
  "type": "module",
  "main": "./dist/google-tag-manager-loader.js",
  "module": "./dist/google-tag-manager-loader.js",
  "types": "./dist/google-tag-manager-loader.d.ts",
  "exports": {
    ".": {
      "import": "./dist/google-tag-manager-loader.js",
      "types": "./dist/google-tag-manager-loader.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "demo:dev": "vite --config vite.demo.config.ts",
    "demo:build": "vite build --config vite.demo.config.ts",
    "demo:preview": "vite preview --config vite.demo.config.ts --outDir dist-demo",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "gtm",
    "google-tag-manager",
    "analytics",
    "tracking"
  ],
  "author": "Zoltan Radics",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoltanradics/google-tag-manager-loader.git"
  },
  "homepage": "https://github.com/zoltanradics/google-tag-manager-loader#readme",
  "bugs": {
    "url": "https://github.com/zoltanradics/google-tag-manager-loader/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.3.0"
  },
  "dependencies": {
    "@zoltanradics/async-script-loader": "^0.1.0"
  }
}
